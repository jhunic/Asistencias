--San Francisco de Cuapa 08 de agosto 2024

select t.*, t.rowid from arckce t where no_cta = '10025800253291' and (cheque = 31787 or cheque = 31789);
select t.*, t.rowid from arckcl t where no_cta = '10025800253291' and (cheque = 31787 or cheque = 31789);
select t.*, t.rowid from arckmm t where no_cta = '10025800253291' and (no_docu = 31787 or no_docu = 31789);
select t.*, t.rowid from arckes t where no_cta = '10025800253291' and (no_secuencia = 5 or no_secuencia = 3);

update fp_eppto set estado = 'P' where eppto_id = 2;
commit;

